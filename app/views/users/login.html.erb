<h1> Login: </h1>
<% if flash[:errors] %>
<%= flash[:errors] %>
<% end %>
<%= form_tag check_login_path, method: :get do %>
    <%= label_tag :username, "Enter your Username: " %>
    <%= text_field_tag :username %><br>
    <%= label_tag :password, "Enter your Password: " %>
    <%= password_field_tag (:password) %><br>
    <%= submit_tag "Submit" %>
<% end %>


